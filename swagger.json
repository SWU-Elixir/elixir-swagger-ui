{
    "openapi": "3.0.1",
    "info": {
      "title": "Elixir API",
      "description": "API documentation for Elixir",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:8080"
      }
    ],
    "tags": [
      {
        "name": "Member API",
        "description": "회원 관련 API"
      },
      {
        "name": "Auth API",
        "description": "회원 인증 관련 API"
      }
    ],
    "paths": {
      "/api/member/signup": {
        "post": {
          "tags": [
            "Member API"
          ],
          "summary": "회원가입",
          "description": "새로운 회원을 등록합니다.",
          "operationId": "signUp",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SignUpRequestDTO"
                }
              }
            },
            "required": true
          },
          "responses": {
            "400": {
              "description": "회원가입 실패",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string",
                    "example": "회원가입 실패: 이메일이 이미 존재합니다."
                  }
                }
              }
            },
            "201": {
              "description": "회원가입 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string",
                    "example": "회원가입 성공 - member_id = 1"
                  }
                }
              }
            }
          }
        }
      },
      "/api/auth/refresh": {
        "post": {
          "tags": [
            "Auth API"
          ],
          "summary": "토큰 재발급",
          "description": "Access Token, Refresh Token을 재발급합니다.",
          "operationId": "refresh",
          "responses": {
            "401": {
              "description": "Access Token 재발급 실패",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string"
                  },
                  "example": "Access Token 재발급 실패: 유효하지 않은 Refresh Token입니다."
                }
              }
            },
            "200": {
              "description": "Access Token 재발급 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/TokenResponseDTO"
                  },
                  "example": {
                    "grantType": "bearer",
                    "accessToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
                    "refreshToken": "dGhpc2lzYXJlZnJlc2h0b2tlbg=="
                  }
                }
              }
            }
          },
          "security": [
            {
              "bearerAuth": []
            }
          ]
        }
      },
      "/api/auth/logout": {
        "post": {
          "tags": [
            "Auth API"
          ],
          "summary": "로그아웃",
          "description": "로그아웃합니다.",
          "operationId": "logout",
          "responses": {
            "400": {
              "description": "로그아웃 실패",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string"
                  },
                  "example": "로그아웃 실패: 이미 만료된 토큰입니다."
                }
              }
            },
            "200": {
              "description": "로그아웃 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string"
                  },
                  "example": "로그아웃 성공"
                }
              }
            }
          },
          "security": [
            {
              "bearerAuth": []
            }
          ]
        }
      },
      "/api/auth/login": {
        "post": {
          "tags": [
            "Auth API"
          ],
          "summary": "로그인",
          "description": "로그인합니다.",
          "operationId": "login",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginRequestDTO"
                }
              }
            },
            "required": true
          },
          "responses": {
            "401": {
              "description": "로그인 실패",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string"
                  },
                  "example": "로그인 실패: 이메일 또는 비밀번호가 일치하지 않습니다."
                }
              }
            },
            "200": {
              "description": "로그인 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "$ref": "#/components/schemas/TokenResponseDTO"
                  },
                  "example": {
                    "grantType": "bearer",
                    "accessToken": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...",
                    "refreshToken": "dGhpc2lzYXJlZnJlc2h0b2tlbg=="
                  }
                }
              }
            }
          }
        }
      },
      "/api/member/check-email": {
        "get": {
          "tags": [
            "Member API"
          ],
          "summary": "이메일 중복 여부 조회",
          "description": "기존 가입 회원과 이메일이 중복되는지 조회합니다.",
          "operationId": "checkEmailDuplicate",
          "parameters": [
            {
              "name": "email",
              "in": "query",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "중복 여부 조회 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "boolean"
                  }
                }
              }
            }
          }
        }
      },
      "/api/member/withdrawal": {
        "delete": {
          "tags": [
            "Member API"
          ],
          "summary": "회원탈퇴",
          "description": "회원 정보를 삭제합니다.",
          "operationId": "withdrawal",
          "responses": {
            "400": {
              "description": "잘못된 요청",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string",
                    "example": "요청이 잘못되었습니다."
                  }
                }
              }
            },
            "200": {
              "description": "회원탈퇴 성공",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string",
                    "example": "회원탈퇴 성공"
                  }
                }
              }
            },
            "500": {
              "description": "서버 오류",
              "content": {
                "*/*": {
                  "schema": {
                    "type": "string",
                    "example": "회원탈퇴 중 오류가 발생했습니다."
                  }
                }
              }
            }
          },
          "security": [
            {
              "bearerAuth": []
            }
          ]
        }
      }
    },
